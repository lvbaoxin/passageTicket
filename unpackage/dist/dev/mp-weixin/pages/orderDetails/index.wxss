
.orderDetailsTop {
	background: #1485EE;
	padding: 40rpx 20rpx;
	display: flex;
	align-items: center;
	justify-content: space-between;
	color: #fff;
}
.orderState {
	font-size: 30rpx;
	margin-bottom: 20rpx;
}
.orderText2 {
	font-size: 24rpx;
}
.orderPrice {
	font-size: 30rpx;
	margin-bottom: 20rpx;
}
.orderPriceDetails {
	font-size: 24rpx;
}
.buyTop {

	border-radius: 10rpx;
	overflow: hidden;
}
.buyInfo {
	background: #fff;
	padding: 30rpx 20rpx;
	color: #000;
	margin: 20rpx 20rpx 0 20rpx;
	border-radius: 10rpx 10rpx 0 0;
}
.buyAbout {
	margin: 0 20rpx;
	font-size: 30rpx;
	text-align: center;
	color: #1485ee;
	padding: 24rpx 0;
	background: #fff;
	border-top: 1rpx solid #A8A8A8;
	border-radius: 0 0 10rpx 10rpx;
}
.ticketListLeft {
	display: flex;
	align-items: center;
	justify-content: space-between;
}
.ticketTime {
	font-size: 32rpx;
	margin-bottom: 14rpx;
	position: relative;
}
.ticketDay {
	font-size: 26rpx;
	margin-bottom: 14rpx;
}
.ticketDuration {
	color: #000;
	font-size: 24rpx;
	margin-top: 10rpx;
	align-items: center;
	text-align: center;
}
.ticketType {
	color: #000;
	text-align: center;
	font-size: 26rpx;
	padding-bottom: 10rpx;
	border-bottom: 1rpx solid #000;
}
.ticketCity {
	font-size: 26rpx;
}
.ticketInfo {
	width: 37%;
}
.collItem1 {
	font-size: 30rpx;
}
.collItem2,
.collItem3 {
	color: #a7a7a7;
	font-size: 24rpx;
	margin-top: 20rpx;
}
.collItem4 {
	text-align: right;
	font-size: 36rpx;
	color: #FF5A00;
}
.collItem5 {
	color: #1485ee;
	font-size: 24rpx;
	margin-left: 10rpx;
}
.orderCollapse .uni-collapse {
	margin: 20rpx;
	padding: 20rpx;
	border-radius: 10rpx;
	width: auto;
}
.orderTitle {
	color: #1485ee;
}
.orderText {
	color: #a7a7a7;
	font-size: 24rpx;
}
.collList {
	border-bottom: 1rpx solid #F6F6F6;
	padding: 20rpx 0;
}
.orderBottomInfo {
	background: #fff;
	padding: 20rpx;
	color: #A7A7A7;
	margin: 20rpx;
	border-radius: 10rpx;
}
.orderInfoTitle {
	border-bottom: 1rpx solid #F6F6F6;
	color: #1485ee;
	height: 80rpx;
	line-height: 80rpx;
}
.orderInfoText {
	color: #a7a7a7;
	font-size: 24rpx;
	margin: 10rpx 0;
}
.dialogBg {
	width: 100%;
	height: 100vh;
	background: rgba(0, 0, 0, 0.2);
	position: absolute;
	top: 0px;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 999;
}
.dialogInside {
	width: 90%;
	margin: 0 auto;
	background: #fff;
	position: absolute;
	left: 50%;
	top: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
	border-radius: 10rpx;
	overflow: hidden;
}
.dialogTop {

	height: 100rpx;
	border-bottom: 1rpx solid #F6F6F6;
	padding: 0 20rpx;
	font-size: 32rpx;
	color: #1a1a1a;
	align-items: center;
	justify-content: space-between;
	display: flex;
}
.dialogBottom {
	height: 100rpx;

	padding: 0 20rpx;
	font-size: 32rpx;
	color: #1a1a1a;
	align-items: center;
	justify-content: space-between;
	display: flex;
}
.dialogOrderList {
	align-items: center;
	justify-content: space-between;
	display: flex;
	margin-bottom: 10rpx;
}
.dialogText {

	padding: 20rpx;
	font-size: 24rpx;
	color: #1a1a1a;
}
.dialogPrice {
	text-align: right;
	font-size: 36rpx;
	color: #FF5A00;
}
.dialogTit {
	width: 40%;
	color: #a7a7a7;
}
.buttomBtn{
	position: -webkit-sticky;
	position: sticky;bottom:0;
	left:0;right:0;
	display:flex;
	align-items: center;
	justify-content: space-between;
}
.buttomBtnItem{
	flex:1;
	text-align:center;
	height:94rpx;
	color:#A8A8A8;
	line-height: 94rpx;
	background:#fff;
	font-size:32rpx;
}
.buttomBlue{
	background:#1485ee;
	color:#fff;
}

