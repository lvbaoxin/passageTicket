.ticketView .uni-collapse {
	background: transparent;
	background: #ffffff;
	border-radius: 10rpx;
	padding: 18rpx;
	margin: 20rpx;
	width: 90%;
}
.ticketBuyAdd {
	display: flex;
	align-items: center;
	justify-content: center;
	color: #1485ee;
	padding:10rpx 0;
}
.ticketTypeList{
	display:flex;align-items: center;
	justify-content: space-between;
}
.ticketTypePic{
	width: 120rpx;
	height: 90rpx;
	border-radius: 5rpx;
}
.ticketTypeTitle{
	font-size: 30rpx;
}
.ticketTypeNum{
color: #1485ee;
	font-size: 24rpx;
	margin-left: 10rpx;
}
.ticketTypeAbout{
	margin-top: 10rpx;
	color: #a7a7a7;
	font-size: 24rpx;
}
.ticketTypePrice{
	text-align: right;
	font-size: 36rpx;
	margin-bottom: 10rpx;
	color: #FF5A00;
}
.ticketTypeBtn{
	font-size: 30rpx;
	width: 120rpx;
	height: 60rpx;
	background: #1485ee;
	border-radius: 5rpx;
	text-align: center;
	line-height: 60rpx;
	color:#fff;
	float: right;
}

